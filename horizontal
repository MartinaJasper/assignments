import java.util.Scanner;

public class Horizontal {
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the number of asterisk to be printed in the line");
        int n= s.nextInt();
        for(int i=1;i<=n;i++)
        {
            System.out.print("*");
        }
    }
}
